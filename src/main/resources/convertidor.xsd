           elementFormDefault="qualified">

    <xs:element name="getPesoDolarRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="importeP" type="xs:long" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getDolarPesoRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="importeD" type="xs:long" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getPesoDolarResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="importeD" type="xs:long" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="getDolarPesoResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="importeP" type="xs:long" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
